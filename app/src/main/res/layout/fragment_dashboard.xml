<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:scrollbars="none">
        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <androidx.appcompat.widget.LinearLayoutCompat
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_4sdp"
                android:paddingHorizontal="@dimen/_6sdp"
                android:orientation="vertical">

                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginVertical="@dimen/_4sdp"
                    android:paddingStart="@dimen/_16sdp"
                    android:paddingEnd="@dimen/_40sdp"
                    android:weightSum="1"
                    android:visibility="visible"
                    android:id="@+id/llAgent">

                    <com.project.supportClasses.VerticalTextview
                        android:layout_width="0dp"
                        android:layout_weight=".1"
                        android:visibility="visible"
                        android:fontFamily="@font/roboto_bold"
                        android:textSize="@dimen/_8ssp"
                        android:textColor="#2A277E"
                        android:gravity="center"
                        android:text="@string/agent"
                        android:layout_height="match_parent"/>


                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_width="0dp"
                        android:layout_weight=".9"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">
                        <com.google.android.material.card.MaterialCardView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:cardBackgroundColor="#141173"
                            app:cardElevation="@dimen/_4sdp"
                            app:cardCornerRadius="@dimen/_8sdp">
                            <androidx.appcompat.widget.LinearLayoutCompat
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <de.hdodenhof.circleimageview.CircleImageView
                                    android:id="@+id/ivAgentPic"
                                    android:layout_width="@dimen/_50sdp"
                                    android:layout_height="@dimen/_40sdp"
                                    android:layout_margin="@dimen/_12sdp"
                                    app:civ_border_width="@dimen/_2sdp"
                                    app:civ_border_color="#03EAE3"
                                    app:civ_circle_background_color="#03EAE3"
                                    android:src="@drawable/ic_employee_blue"
                                    />

                                <androidx.appcompat.widget.LinearLayoutCompat
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical"
                                    android:layout_gravity="center_vertical"
                                    android:gravity="center_vertical">

                                    <androidx.appcompat.widget.AppCompatTextView
                                        android:layout_width="wrap_content"
                                        android:id="@+id/tvAgentName"
                                        tools:text="SURESH"
                                        android:textSize="@dimen/_12ssp"
                                        android:fontFamily="@font/roboto_bold"
                                        android:textColor="@color/white"
                                        android:layout_height="wrap_content"/>

                                    <androidx.appcompat.widget.AppCompatTextView
                                        android:layout_width="wrap_content"
                                        android:id="@+id/tvBranchName"
                                        tools:text="Salt Lake Branch"
                                        android:textSize="@dimen/_10ssp"
                                        android:fontFamily="@font/roboto_bold"
                                        android:textColor="#03EAE3"
                                        android:layout_height="wrap_content"/>

                                    <androidx.appcompat.widget.AppCompatTextView
                                        android:layout_width="wrap_content"
                                        android:id="@+id/tvAgentCode1"
                                        tools:text="Salt Lake Branch"
                                        android:textSize="@dimen/_10ssp"
                                        android:fontFamily="@font/roboto_bold"
                                        android:textColor="#03EAE3"
                                        android:layout_height="wrap_content"/>

                                </androidx.appcompat.widget.LinearLayoutCompat>

                            </androidx.appcompat.widget.LinearLayoutCompat>

                        </com.google.android.material.card.MaterialCardView>

                    </androidx.appcompat.widget.LinearLayoutCompat>


                </androidx.appcompat.widget.LinearLayoutCompat>

                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginVertical="@dimen/_4sdp"
                    android:paddingStart="@dimen/_16sdp"
                    android:paddingEnd="@dimen/_40sdp"
                    android:visibility="visible"
                    android:id="@+id/llEmployee"
                    android:weightSum="1">

                    <com.project.supportClasses.VerticalTextview
                        android:layout_width="0dp"
                        android:layout_weight=".1"
                        android:visibility="visible"
                        android:fontFamily="@font/roboto_bold"
                        android:textSize="@dimen/_8ssp"
                        android:textColor="#2A277E"
                        android:gravity="center"
                        app:textAllCaps="true"
                        android:text="@string/introducer"
                        android:layout_height="match_parent"/>


                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_width="0dp"
                        android:layout_weight=".9"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">
                        <com.google.android.material.card.MaterialCardView
                            android:id="@+id/cvAgent"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:cardBackgroundColor="#03EAE3"
                            app:cardElevation="@dimen/_4sdp"
                            app:cardCornerRadius="@dimen/_8sdp">
                            <androidx.appcompat.widget.LinearLayoutCompat
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <de.hdodenhof.circleimageview.CircleImageView
                                    android:id="@+id/ivIntroducerPic"
                                    android:layout_width="@dimen/_50sdp"
                                    android:layout_height="@dimen/_40sdp"
                                    android:layout_margin="@dimen/_12sdp"
                                    app:civ_border_width="@dimen/_2sdp"
                                    app:civ_border_color="#141173"
                                    app:civ_circle_background_color="#141173"
                                    android:src="@drawable/ic_employee_white"
                                    />

                                <androidx.appcompat.widget.LinearLayoutCompat
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical"
                                    android:layout_gravity="center_vertical"
                                    android:gravity="center_vertical">

                                    <androidx.appcompat.widget.AppCompatTextView
                                        android:layout_width="wrap_content"
                                        android:id="@+id/tvIntroducerName"
                                        tools:text="KAMLESH"
                                        android:textSize="@dimen/_12ssp"
                                        android:fontFamily="@font/roboto_bold"
                                        android:textColor="@color/white"
                                        android:layout_height="wrap_content"/>
                                    <androidx.appcompat.widget.AppCompatTextView
                                        android:layout_width="wrap_content"
                                        android:id="@+id/tv_branch_name1"
                                        tools:text="Agent Code #3562"
                                        android:textSize="@dimen/_10ssp"
                                        android:fontFamily="@font/roboto_bold"
                                        android:textColor="#141173"
                                        android:layout_height="wrap_content"/>

                                    <androidx.appcompat.widget.AppCompatTextView
                                        android:layout_width="wrap_content"
                                        android:id="@+id/tvAgentCode"
                                        tools:text="Agent Code #3562"
                                        android:textSize="@dimen/_10ssp"
                                        android:fontFamily="@font/roboto_bold"
                                        android:textColor="#141173"
                                        android:layout_height="wrap_content"/>




                                </androidx.appcompat.widget.LinearLayoutCompat>

                            </androidx.appcompat.widget.LinearLayoutCompat>

                        </com.google.android.material.card.MaterialCardView>

                    </androidx.appcompat.widget.LinearLayoutCompat>


                </androidx.appcompat.widget.LinearLayoutCompat>

            </androidx.appcompat.widget.LinearLayoutCompat>


            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="@dimen/_6sdp"
                android:paddingBottom="@dimen/_15sdp"
                android:clipToPadding="false"
                android:paddingHorizontal="@dimen/_35sdp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent">

                <com.google.android.material.card.MaterialCardView
                    android:id="@+id/materialCardView3"
                    android:layout_width="@dimen/_100sdp"
                    android:layout_height="@dimen/_90sdp"
                    android:layout_marginBottom="@dimen/_6sdp"
                    android:background="@drawable/dashboaed_tiles"
                    app:cardCornerRadius="@dimen/_8sdp"
                    app:cardElevation="@dimen/_4sdp"
                    app:layout_constraintBottom_toTopOf="@+id/materialCardView4"
                    app:layout_constraintEnd_toStartOf="@+id/materialCardView2"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintHorizontal_chainStyle="spread"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.0">

                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:paddingHorizontal="@dimen/_16sdp">

                        <androidx.appcompat.widget.AppCompatImageView
                            android:id="@+id/ivAttendance"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/_12sdp"
                            android:src="@drawable/attendance"
                            app:civ_circle_background_color="#141173" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/tvAttendance"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/roboto_bold"
                            android:text="@string/attendance"
                            android:textColor="#141173"
                            android:textSize="@dimen/_8ssp"
                            app:textAllCaps="true" />


                    </androidx.appcompat.widget.LinearLayoutCompat>

                </com.google.android.material.card.MaterialCardView>

                <com.google.android.material.card.MaterialCardView
                    android:id="@+id/materialCardView2"
                    android:layout_width="@dimen/_100sdp"
                    android:layout_height="@dimen/_90sdp"
                    android:layout_marginVertical="@dimen/_6sdp"
                    android:background="@drawable/dashboaed_tiles"
                    app:cardCornerRadius="@dimen/_8sdp"
                    app:cardElevation="@dimen/_4sdp"
                    app:layout_constraintBottom_toBottomOf="@+id/materialCardView3"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/materialCardView3"
                    app:layout_constraintTop_toTopOf="@+id/materialCardView3">

                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:paddingHorizontal="@dimen/_16sdp">

                        <androidx.appcompat.widget.AppCompatImageView
                            android:id="@+id/ivCustomer"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_margin="@dimen/_12sdp"
                            android:src="@drawable/customer"
                            app:civ_circle_background_color="#141173" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/tvCustomer"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="bottom|center"
                            android:fontFamily="@font/roboto_bold"
                            android:gravity="center"
                            android:text="@string/customer"
                            android:textColor="#141173"
                            android:textSize="@dimen/_8ssp"
                            app:textAllCaps="true" />


                    </androidx.appcompat.widget.LinearLayoutCompat>

                </com.google.android.material.card.MaterialCardView>

                <com.google.android.material.card.MaterialCardView
                    android:id="@+id/materialCardView4"
                    android:layout_width="@dimen/_100sdp"
                    android:layout_height="@dimen/_90sdp"
                    android:layout_marginVertical="@dimen/_8sdp"
                    android:background="@drawable/dashboaed_tiles"
                    app:cardCornerRadius="@dimen/_8sdp"
                    app:cardElevation="@dimen/_4sdp"
                    app:layout_constraintEnd_toStartOf="@+id/materialCardView5"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintHorizontal_chainStyle="spread"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/materialCardView3">

                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:paddingHorizontal="@dimen/_16sdp">

                        <androidx.appcompat.widget.AppCompatImageView
                            android:id="@+id/ivAvailablePlan"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/_12sdp"
                            android:src="@drawable/plan"
                            app:civ_circle_background_color="#141173" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/tvAvailablePlan"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="bottom"
                            android:ellipsize="end"
                            android:fontFamily="@font/roboto_bold"
                            android:gravity="center"
                            android:text="@string/available_plan"
                            android:textColor="#141173"
                            android:textSize="@dimen/_8ssp"
                            app:textAllCaps="true" />


                    </androidx.appcompat.widget.LinearLayoutCompat>

                </com.google.android.material.card.MaterialCardView>

                <com.google.android.material.card.MaterialCardView
                    android:id="@+id/materialCardView5"
                    android:layout_width="@dimen/_100sdp"
                    android:layout_height="@dimen/_90sdp"
                    android:layout_marginVertical="@dimen/_8sdp"
                    android:background="@drawable/dashboaed_tiles"
                    app:cardCornerRadius="@dimen/_8sdp"
                    app:cardElevation="@dimen/_4sdp"
                    app:layout_constraintBottom_toBottomOf="@+id/materialCardView4"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/materialCardView4"
                    app:layout_constraintTop_toTopOf="@+id/materialCardView4">

                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:orientation="vertical"
                        android:paddingHorizontal="@dimen/_16sdp">

                        <androidx.appcompat.widget.AppCompatImageView
                            android:id="@+id/ivTeam"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/_12sdp"
                            android:layout_gravity="center"
                            android:src="@drawable/team"
                            app:civ_circle_background_color="#141173" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/tvTeam"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="bottom|center"
                            android:fontFamily="@font/roboto_bold"
                            android:gravity="center"
                            android:text="@string/team"
                            android:textColor="#141173"
                            android:textSize="@dimen/_8ssp"
                            app:textAllCaps="true" />


                    </androidx.appcompat.widget.LinearLayoutCompat>

                </com.google.android.material.card.MaterialCardView>

                <com.google.android.material.card.MaterialCardView
                    android:id="@+id/materialCardView6"
                    android:layout_width="@dimen/_100sdp"
                    android:layout_height="@dimen/_90sdp"
                    android:layout_marginVertical="@dimen/_8sdp"
                    android:background="@drawable/dashboaed_tiles"
                    app:cardCornerRadius="@dimen/_8sdp"
                    app:cardElevation="@dimen/_4sdp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintHorizontal_chainStyle="spread"
                    app:layout_constraintStart_toStartOf="@+id/materialCardView4"
                    app:layout_constraintTop_toBottomOf="@+id/materialCardView4"
                    app:layout_constraintVertical_bias="0.0">

                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:paddingHorizontal="@dimen/_16sdp"
                        android:paddingVertical="@dimen/_12sdp">

                        <androidx.appcompat.widget.AppCompatImageView
                            android:id="@+id/ivBusinessReport"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/_8sdp"
                            android:src="@drawable/ic_briefcase"
                            app:civ_circle_background_color="#141173" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/tvBusinessReport"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="bottom"
                            android:ellipsize="end"
                            android:fontFamily="@font/roboto_bold"
                            android:gravity="center"
                            android:text="@string/business_report"
                            android:textColor="#141173"
                            android:textSize="@dimen/_8ssp"
                            app:textAllCaps="true" />


                    </androidx.appcompat.widget.LinearLayoutCompat>

                </com.google.android.material.card.MaterialCardView>


                <com.google.android.material.card.MaterialCardView
                    android:id="@+id/materialCardView"
                    android:layout_width="@dimen/_100sdp"
                    android:layout_height="@dimen/_90sdp"
                    android:layout_marginVertical="@dimen/_8sdp"
                    android:background="@drawable/dashboaed_tiles"
                    app:cardCornerRadius="@dimen/_8sdp"
                    app:cardElevation="@dimen/_4sdp"
                    app:layout_constraintEnd_toEndOf="@+id/materialCardView5"
                    app:layout_constraintHorizontal_bias="1.0"
                    app:layout_constraintBottom_toTopOf="@+id/agentJoiningCard"
                    app:layout_constraintHorizontal_chainStyle="spread"
                    app:layout_constraintStart_toEndOf="@+id/materialCardView6"
                    app:layout_constraintTop_toBottomOf="@+id/materialCardView5">

                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:paddingHorizontal="@dimen/_16sdp"
                        android:paddingVertical="@dimen/_8sdp">

                        <androidx.appcompat.widget.AppCompatImageView
                            android:id="@+id/ivLeave"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/_8sdp"
                            android:src="@drawable/ic_leave"
                            app:civ_circle_background_color="#141173" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/tvLeave"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="bottom"
                            android:ellipsize="end"
                            android:fontFamily="@font/roboto_bold"
                            android:gravity="center"
                            android:text="@string/leave"
                            android:textColor="#141173"
                            android:textSize="@dimen/_8ssp"
                            app:textAllCaps="true" />


                    </androidx.appcompat.widget.LinearLayoutCompat>

                </com.google.android.material.card.MaterialCardView>

                <com.google.android.material.card.MaterialCardView
                    android:id="@+id/agentJoiningCard"
                    android:layout_width="@dimen/_100sdp"
                    android:layout_height="@dimen/_90sdp"
                    android:layout_marginVertical="@dimen/_8sdp"
                    android:background="@drawable/dashboaed_tiles"
                    app:cardCornerRadius="@dimen/_8sdp"
                    app:cardElevation="@dimen/_4sdp"
                    app:layout_constraintEnd_toEndOf="@+id/materialCardView"
                    app:layout_constraintHorizontal_bias="1.0"
                    app:layout_constraintHorizontal_chainStyle="spread"
                    app:layout_constraintStart_toEndOf="@+id/materialCardViewMisReport"
                    app:layout_constraintTop_toBottomOf="@+id/materialCardView">

                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:paddingHorizontal="@dimen/_16sdp"
                        android:paddingVertical="@dimen/_8sdp">

                        <androidx.appcompat.widget.AppCompatImageView
                            android:id="@+id/iv_agent_joining"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/_8sdp"
                            android:src="@drawable/ic_leave"
                            app:civ_circle_background_color="#141173" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/tv_agent_joining"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="bottom"
                            android:ellipsize="end"
                            android:fontFamily="@font/roboto_bold"
                            android:gravity="center"
                            android:text="@string/agent_joining"
                            android:textColor="#141173"
                            android:textSize="@dimen/_8ssp"
                            app:textAllCaps="true" />


                    </androidx.appcompat.widget.LinearLayoutCompat>

                </com.google.android.material.card.MaterialCardView>


                <com.google.android.material.card.MaterialCardView
                    android:id="@+id/materialCardViewMisReport"
                    android:layout_width="@dimen/_100sdp"
                    android:layout_height="@dimen/_90sdp"
                    android:background="@drawable/dashboaed_tiles"
                    app:cardCornerRadius="@dimen/_8sdp"
                    android:layout_marginVertical="@dimen/_8sdp"
                    app:cardElevation="@dimen/_4sdp"
                    app:layout_constraintEnd_toEndOf="@+id/materialCardView6"
                    app:layout_constraintHorizontal_chainStyle="spread"
                    app:layout_constraintStart_toStartOf="@+id/materialCardView6"
                    app:layout_constraintTop_toBottomOf="@+id/materialCardView6">

                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:paddingHorizontal="@dimen/_16sdp">

                        <androidx.appcompat.widget.AppCompatImageView
                            android:id="@+id/ivMisReport"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/_12sdp"
                            android:src="@drawable/plan"
                            app:civ_circle_background_color="#141173" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/tvMisReport"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/roboto_bold"
                            android:text="@string/mis_report"
                            android:textColor="#141173"
                            android:textSize="@dimen/_8ssp"
                            app:textAllCaps="true" />


                    </androidx.appcompat.widget.LinearLayoutCompat>

                </com.google.android.material.card.MaterialCardView>


                <com.google.android.material.card.MaterialCardView
                    android:id="@+id/materialCardViewMessage"
                    android:layout_width="@dimen/_100sdp"
                    android:layout_height="@dimen/_90sdp"
                    android:background="@drawable/dashboaed_tiles"
                    app:cardCornerRadius="@dimen/_8sdp"
                    android:layout_marginVertical="@dimen/_8sdp"
                    app:cardElevation="@dimen/_4sdp"
                    app:layout_constraintEnd_toEndOf="@+id/materialCardViewMisReport"
                    app:layout_constraintHorizontal_chainStyle="spread"
                    app:layout_constraintStart_toStartOf="@+id/materialCardViewMisReport"
                    app:layout_constraintTop_toBottomOf="@+id/materialCardViewMisReport">

                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:paddingHorizontal="@dimen/_16sdp">

                        <androidx.appcompat.widget.AppCompatImageView
                            android:id="@+id/ivMisReport1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/_12sdp"
                            android:src="@drawable/plan"
                            app:civ_circle_background_color="#141173" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/tvMisReport1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/roboto_bold"
                            android:text="@string/message"
                            android:textColor="#141173"
                            android:textSize="@dimen/_8ssp"
                            app:textAllCaps="true" />


                    </androidx.appcompat.widget.LinearLayoutCompat>

                </com.google.android.material.card.MaterialCardView>

            </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.appcompat.widget.LinearLayoutCompat>

    </androidx.core.widget.NestedScrollView>
</androidx.appcompat.widget.LinearLayoutCompat>